{
  "multipart": [
    {   "when": { "up": "true" },
      "apply": { "model": "structurize:block/bricks/blockbrownbrickwallpost" }
    },
    {   "when": { "north": "low" },
      "apply": { "model": "structurize:block/bricks/blockbrownbrickwallside", "uvlock": true }
    },
    {   "when": { "east": "low" },
      "apply": { "model": "structurize:block/bricks/blockbrownbrickwallside", "y": 90, "uvlock": true }
    },
    {   "when": { "south": "low" },
      "apply": { "model": "structurize:block/bricks/blockbrownbrickwallside", "y": 180, "uvlock": true }
    },
    {   "when": { "west": "low" },
      "apply": { "model": "structurize:block/bricks/blockbrownbrickwallside", "y": 270, "uvlock": true }
    },
    {
      "when": {
        "north": "tall"
      },
      "apply": {
        "model": "structurize:block/bricks/blockbrownbrickwallsidetall",
        "uvlock": true
      }
    },
    {
      "when": {
        "east": "tall"
      },
      "apply": {
        "model": "structurize:block/bricks/blockbrownbrickwallsidetall",
        "y": 90,
        "uvlock": true
      }
    },
    {
      "when": {
        "south": "tall"
      },
      "apply": {
        "model": "structurize:block/bricks/blockbrownbrickwallsidetall",
        "y": 180,
        "uvlock": true
      }
    },
    {
      "when": {
        "west": "tall"
      },
      "apply": {
        "model": "structurize:block/bricks/blockbrownbrickwallsidetall",
        "y": 270,
        "uvlock": true
      }
    }
  ]
}